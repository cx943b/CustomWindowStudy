    <Color x:Key="PrimaryColor">#FF6495ED</Color>
    <Color x:Key="DarkPrimaryColor">#FF3465BD</Color>
    <Color x:Key="SecondaryColor">#FFFF69B4</Color>
    <Color x:Key="AlertColor">#FFDC143C</Color>
    <Color x:Key="WarningColor">#FFFFD700</Color>
    <Color x:Key="SuccessColor">#FF228B22</Color>
    <Color x:Key="ForegroundColor">#EEE</Color>
    <Color x:Key="ListBoxItemColor">#FF969696</Color>

    <!--GrayColorStep040-->
    <Color x:Key="ControlDarkBorderColor">#FF282828</Color>
    <!--GrayColorStep060-->
    <Color x:Key="ControlBorderColor">#FF3C3C3C</Color>
    <!--GrayColorStep080-->
    <Color x:Key="ControlLightBorderColor">#FF505050</Color>

    <!--GrayColorStep060-->
    <Color x:Key="ControlDeepDarkBackgroundColor">#FF3C3C3C</Color>
    <!--GrayColorStep080-->
    <Color x:Key="ControlDarkBackgroundColor">#FF505050</Color>
    <!--GrayColorStep100-->
    <Color x:Key="ControlBackgroundColor">#FF646464</Color>
    <!--GrayColorStep120-->
    <Color x:Key="ControlLightBackgroundColor">#FF787878</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}"/>
    <SolidColorBrush x:Key="AlertBrush" Color="{StaticResource AlertColor}"/>
    <SolidColorBrush x:Key="WarningBrush" Color="{StaticResource WarningColor}"/>
    <SolidColorBrush x:Key="SuccessBrush" Color="{StaticResource SuccessColor}"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource ForegroundColor}"/>
    <SolidColorBrush x:Key="ControlGlowBorderBrush" Opacity="0.3" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="ListBoxItemBrush" Color="{StaticResource ListBoxItemColor}"/>

    <!-- BorderBrushes -->
    <SolidColorBrush x:Key="ControlDarkBorderBrush" Color="{StaticResource ControlDarkBorderColor}"/>
    <SolidColorBrush x:Key="ControlBorderBrush" Color="{StaticResource ControlBorderColor}"/>
    <SolidColorBrush x:Key="ControlLightBorderBrush" Color="{StaticResource ControlLightBorderColor}"/>

    <!-- BackgroundBrushes -->
    <SolidColorBrush x:Key="ControlDeepDarkBackgroundBrush" Color="{StaticResource ControlDeepDarkBackgroundColor}"/>
    <SolidColorBrush x:Key="ControlDarkBackgroundBrush" Color="{StaticResource ControlDarkBackgroundColor}"/>
    <SolidColorBrush x:Key="ControlBackgroundBrush" Color="{StaticResource ControlBackgroundColor}"/>
    <SolidColorBrush x:Key="ControlLightBackgroundBrush" Color="{StaticResource ControlLightBackgroundColor}"/>

    <LinearGradientBrush x:Key="PrimaryGradientBrush" StartPoint="0 0" EndPoint="0 1">
        <GradientBrush.GradientStops>
            <GradientStop Color="{StaticResource PrimaryColor}" Offset="0"/>
            <GradientStop Color="{StaticResource DarkPrimaryColor}" Offset="1"/>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ControlBackgroundGradientBrush" StartPoint="0.5 0" EndPoint="0.5 1">
        <GradientBrush.GradientStops>
            <GradientStop Color="{StaticResource GrayColorStep105}" Offset="0"/>
            <GradientStop Color="{StaticResource GrayColorStep085}" Offset="1"/>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ControlLightBackgroundGradientBrush" StartPoint="0.5 0" EndPoint="0.5 1">
        <GradientBrush.GradientStops>
            <GradientStop Color="{StaticResource GrayColorStep125}" Offset="0"/>
            <GradientStop Color="{StaticResource GrayColorStep105}" Offset="1"/>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ControlDarkBackgroundGradientBrush" StartPoint="0.5 0" EndPoint="0.5 1">
        <GradientBrush.GradientStops>
            <GradientStop Color="{StaticResource GrayColorStep085}" Offset="0"/>
            <GradientStop Color="{StaticResource GrayColorStep065}" Offset="1"/>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>
</ResourceDictionary>